{
  "name": "covalent-electron",
  "version": "0.0.1",
  "private": true,
  "description": "Teradata Electron Platform built on Teradata Covalent",
  "keywords": [
    "angular",
    "covalent",
    "electron",
    "desktop",
    "javascript"
  ],
  "scripts": {
    "lint": "tslint -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
    "ngbuild": "rimraf ./dist && ng build",
    "npm-install": "npm --prefix ./dist install ./dist --production",
    "reinstall": "rimraf node_modules dist dist-ng dist-app && npm i",
    "build": "gulp copy --option noreload",
    "package": "rimraf ./dist-app && npm run ngbuild && npm run build && npm run npm-install && node \"./node_modules/electron-packager/bin/electron-packager.js\" dist Covalent --icon=src/app/assets/ico/icon.icns --out=dist-app --overwrite",
    "live-reload": "rimraf ./dist && rimraf ./dist-ng && gulp watch",
    "test": "rimraf ./dist && rimraf ./dist-ng && npm run build && npm run npm-install && ng test --codeCoverage=true --watch=false --sourceMap=false"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">3",
    "yarn": ">=1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teradata/covalent/covalent-electron.git"
  },
  "bugs": {
    "url": "https://github.com/teradata/covalent/issues"
  },
  "license": "MIT",
  "author": "Teradata UX",
  "contributors": [
    "Kyle Ledbetter <kyle.ledbetter@teradata.com>",
    "Richa Vyas <richa.vyas@teradata.com>",
    "Ed Morales <eduardo.morales@teradata.com>",
    "Jason Weaver <jason.weaver@teradata.com>",
    "Jeremy Wilken <jeremy.wilken@teradata.com>",
    "Jeremy Smartt <jeremy.smartt@teradata.com>"
  ],
  "dependencies": {
    "@angular/animations": "^9.1.2",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "^9.1.2",
    "@angular/compiler": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/forms": "^9.1.2",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "^9.1.2",
    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/platform-server": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@covalent/code-editor": "git+https://github.com/Teradata/covalent-code-editor-nightly.git",
    "@covalent/core": "3.1.0-beta.1",
    "@covalent/highlight": "3.1.0-beta.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.4.1",
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "^9.1.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "3.5.10",
    "@types/node": "^12.11.1",
    "@types/selenium-webdriver": "^4.0.9",
    "chokidar": "^3.3.1",
    "codelyzer": "^5.1.2",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-connect": "^0.6.3",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "gulp": "3.9.1",
    "gulp-help": "1.6.1",
    "jasmine-core": "~3.5.0",
    "karma": "~6.3.16",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-electron": "^6.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "monaco-editor-webpack-plugin": "1.9.0",
    "node-sass": "4.13.1",
    "require-dir": "1.2.0",
    "rimraf": "3.0.2",
    "run-sequence": "^2.2.1",
    "semver": "7.3.2",
    "style-loader": "^1.2.1",
    "ts-node": "^8.8.2",
    "tslint": "5.2.0",
    "typescript": "~3.8.3"
  }
}
